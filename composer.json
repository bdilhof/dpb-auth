{
  "name": "dpb/auth",
  "description": "Authentication for Laravel 10.0",
  "homepage": "https://github.com/bdilhof/dpb-auth",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Bruno Dilhof",
      "email": "dilhof.bruno@dpb.sk",
      "role": "Web Application Developer"
    }
  ],
  "require": {
    "php": "^8",
    "illuminate/support": "^10.0",
    "dpb/ui": "dev-main"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Dpb\\Auth\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dpb\\Auth\\AuthServiceProvider"
      ]
    }
  }
}
